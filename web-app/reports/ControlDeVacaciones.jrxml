	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="EJERCICIO" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT substr(s.periodicidad,1,1) as per,u.clave as ubicacion,e.id,e.clave,concat(ifnull(e.apellido_paterno,''),' ',ifnull(e.apellido_materno,''),' ',ifnull(e.nombres,'')) as nombre,e.alta,e.status,b.fecha as baja
,round((TO_DAYS((case when b.fecha is null or b.fecha<e.alta then current_date else b.fecha end))-TO_DAYS(e.alta))/(365),2) AS anios
,c.antiguedad_years,c.dias_trasladados,c.dias_vacaciones,c.dias_tomados,c.dias_pagados,(c.dias_trasladados+c.dias_vacaciones-c.dias_tomados-c.dias_pagados) as saldo
,(SELECT count(*) FROM vacaciones v join vacaciones_dias d on(v.id=d.vacaciones_id) where v.empleado_id=e.id and year(d.dias_date) =$P{EJERCICIO} ) as diasTmNom
,c.dias_tomados-(SELECT count(*) FROM vacaciones v join vacaciones_dias d on(v.id=d.vacaciones_id) where v.empleado_id=e.id and year(d.dias_date) =$P{EJERCICIO} ) as diasDif
FROM control_de_vacaciones c
join empleado e on (c.empleado_id=e.id) join salario s on(s.empleado_id=e.id)
 join perfil_de_empleado p on (p.empleado_id=e.id) join ubicacion u on (p.ubicacion_id=u.id)
left join baja_de_empleado b on(b.empleado_id=e.id)
 where c.ejercicio=$P{EJERCICIO}
 group by e.id
 order by U.ID,e.alta]]></queryString>

