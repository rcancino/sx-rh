	<queryString><![CDATA[SELECT e.id,e.clave,P.numero_de_trabajador,CONCAT(ifnull(e.apellido_paterno,'')," ",ifnull(e.apellido_materno,'')," ",e.nombres) AS nombre
,(SELECT U.descripcion FROM ubicacion U WHERE p.UBICACION_ID=U.ID) AS ubicacion,i.ejercicio,i.mes,i.tipo,i.calificacion
,i.tasa_bono1,i.tasa_bono2,(case when i.otorgado is true then 'SI' ELSE 'NO' END) otorgado
,i.ingreso_base,i.incentivo,i.checadas_faltantes,i.faltas,i.minutos_no_laborados
,(SELECT x.nombre FROM empresa x ) as EMPRESA
FROM incentivo i join empleado e on(e.id=i.empleado_id) join perfil_de_empleado p on(e.id=p.empleado_id)
where i.tipo=$P{TIPO} and i.ejercicio=$P{EJERCICIO} and i.mes=$P{MES}]]></queryString>

