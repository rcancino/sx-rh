	<queryString><![CDATA[select e.id,e.clave,CONCAT(IFNULL(e.apellido_paterno,''),' ',e.apellido_materno,' ',e.nombres) as nombre,p.numero_de_trabajador,s.periodicidad,x.clave as puesto,u.descripcion as ubicacion,e.alta,e.status,P.numero_de_trabajador as numTrab
,ROUND((-(TIMESTAMPDIFF(MINUTE,DATE(a.fecha_final),e.ALTA)/60)/24)/365,2)  AS years
,ROUND(-(TIMESTAMPDIFF(MINUTE,DATE(a.fecha_final),e.ALTA)/60)/24,0)  AS dias
,a.id,a.ejercicio,a.fecha_inicial,a.fecha_final,a.salario,faltas,incapacidades,faltas_incapacidades_factor,retardos,retardo_factor,productividad,productividad_factor
,monto_bono_total,factor_para_bono,ptu,bono_inicial,porcentaje_bono,bono_preliminar,bono
,a.total_gravable,a.promedio_gravable,a.sueldo_mensual,a.proporcion_promedio_mensual
,a.isr_mensual,a.isr_promedio,a.dif_isr_mensual_promedio,a.tasa,a.isr_por_retener,a.subsidio,a.isrosubsidio,a.resultado_isr_subsidio,a.tabla_normal_isr_sub,a.beneficio_perjuicio,a.isr_ejer_ant
,a.sub_total,a.pensiona,a.otras_ded ,a.prestamo,a.neto_pagado
from BONO a
join empleado e on(e.id=a.empleado_id)
join perfil_de_empleado p on(p.empleado_id=e.id)
join salario s on(s.empleado_id=e.id)
join ubicacion u on(u.id=p.ubicacion_id)
join puesto x on(x.id=p.puesto_id)
where a.ejercicio=$P{EJERCICIO}
]]></queryString>

