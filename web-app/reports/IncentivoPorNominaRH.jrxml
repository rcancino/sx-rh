	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="CAL_DET_ID" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT (SELECT d.folio FROM asistencia a join calendario_det d on(d.id=a.calendario_det_id) where a.id=i.asistencia_id) as folio
,e.id,e.clave,P.numero_de_trabajador,CONCAT(ifnull(e.apellido_paterno,'')," ",ifnull(e.apellido_materno,'')," ",e.nombres) AS nombre
,(SELECT U.descripcion FROM ubicacion U WHERE p.UBICACION_ID=U.ID) AS ubicacion,i.ejercicio,i.mes,i.tipo,i.calificacion
,i.tasa_bono1,i.tasa_bono2,(case when i.otorgado is true then 'SI' ELSE 'NO' END) otorgado
,i.ingreso_base,i.incentivo,i.checadas_faltantes,i.faltas,i.minutos_no_laborados
,(SELECT A.retardo_menor FROM asistencia a join calendario_det d on(d.id=a.calendario_det_id) where a.id=i.asistencia_id) AS ret_mn
,(SELECT A.retardo_mayor FROM asistencia a join calendario_det d on(d.id=a.calendario_det_id) where a.id=i.asistencia_id) AS ret_my
,(SELECT A.retardo_menor_comida FROM asistencia a join calendario_det d on(d.id=a.calendario_det_id) where a.id=i.asistencia_id) AS com_mn
,(SELECT A.retardo_comida FROM asistencia a join calendario_det d on(d.id=a.calendario_det_id) where a.id=i.asistencia_id) AS com_my
FROM incentivo i join empleado e on(e.id=i.empleado_id) join perfil_de_empleado p on(e.id=p.empleado_id)
where i.tipo in('SEMANAL','QUINCENAL') and (SELECT d.id FROM asistencia a join calendario_det d on(d.id=a.calendario_det_id) where a.id=i.asistencia_id)=$P{CAL_DET_ID}]]></queryString>

