	</parameter>
	<parameter name="NomReporte" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["BONO (ASISTENCIA Y PUNTUALIDAD)"]]></defaultValueExpression>
	</parameter>
	<parameter name="EJERCICIO" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT E.id,CONCAT(ifnull(E.apellido_paterno,'')," ",ifnull(E.apellido_materno,'')," ",E.nombres) AS nombre,e.alta,s.periodicidad,P.numero_de_trabajador as numTrab
,(SELECT U.clave FROM puesto U WHERE p.PUESTO_ID=U.ID) AS puesto
,(SELECT U.descripcion FROM ubicacion U WHERE p.UBICACION_ID=U.ID) AS ubicacion
,ROUND(-(TIMESTAMPDIFF(MINUTE,a.fecha_final,e.ALTA)/60)/24,0)+1  AS antDias
,ejercicio,fecha_inicial,fecha_final,a.tipo,(incapacidades+faltas) as faltas_incap,faltas_incapacidades_factor,retardos,retardo_factor,productividad,productividad_factor
,monto_bono_total,factor_para_bono,ptu,bono_inicial,porcentaje_bono,bono_preliminar,bono  
FROM BONO a
join empleado e on(e.id=a.empleado_id)
JOIN perfil_de_empleado p ON(E.ID=p.empleado_id)
join salario s on(s.empleado_id=e.id)
WHERE A.EJERCICIO=$P{EJERCICIO}
]]></queryString>

