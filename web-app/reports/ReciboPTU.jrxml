	<parameter name="IMPORTE_LETRA" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT E.id,E.clave,CONCAT(ifnull(E.apellido_paterno,'')," ",ifnull(E.apellido_materno,'')," ",E.nombres) AS nombre,(SELECT U.descripcion FROM ubicacion U WHERE p.UBICACION_ID=U.ID) AS ubicacion
,n.id,(n.ejercicio-1) as ejercicio,n.periodicidad as tipoPrdo,n.pago,ne.id,ptd.id as ptu_id,ptd.monto_dias,ptd.monto_salario,round(ptd.isr_por_retener,2) as isr_por_retener,ptd.isr_acreditable,ptd.pensiona,ptd.otras_ded,ptd.prestamo,ptd.por_pagar_neto
,(SELECT x.nombre FROM empresa x ) as EMPRESA
FROM ptu_det ptd
join nomina_por_empleado ne on(ptd.nomina_por_empleado_id=ne.id)
join nomina n on(n.id=ne.nomina_id)
join empleado e on(e.id=ne.empleado_id)
join perfil_de_empleado p on(p.empleado_id=e.id)
where NE.ID=$P{NE_ID}]]></queryString>

