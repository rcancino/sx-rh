	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="NomReporte" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["REPORTE DE FALTAS E INCAPACIDADES"]]></defaultValueExpression>
	</parameter>
	<parameter name="EMPLEADO_ID" isForPrompting="true" class="java.lang.Long"/>
	<parameter name="EJERCICIO" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT
C.EJERCICIO, 
U.CLAVE AS ubicacion,concat(ifnull(e.apellido_paterno,''),' ',ifnull(e.apellido_materno,''),' ',ifnull(e.nombres,'')) AS nombre,E.CLAVE AS EMP,P.numero_de_trabajador,S.FECHA,DAYOFWEEK(S.FECHA)AS DIA
,(case when dayofweek(S.fecha)=1 then 'DOM' when dayofweek(S.fecha)=2 then 'LUN' when dayofweek(S.fecha)=3 then 'MAR' when dayofweek(S.fecha)=4 then 'MIE' when dayofweek(S.fecha)=5 
then 'JUE' when dayofweek(S.fecha)=6 then 'VIE' when dayofweek(S.fecha)=7 then 'SAB' ELSE 'ND' END) as diaSem
,s.tipo
,(SELECT I.referencia_imms FROM incapacidad I WHERE I.empleado_id=E.ID AND S.FECHA BETWEEN I.fecha_inicial AND I.fecha_final ) AS REFERENCIA
,(SELECT x.nombre FROM empresa x ) as EMPRESA
FROM CALENDARIO C JOIN calendario_det D ON (C.ID=D.calendario_id) 
JOIN asistencia A  ON ( A.calendario_det_id=D.ID ) JOIN asistencia_det S ON (S.asistencia_id= A.ID )
JOIN empleado E ON (E.ID=A.empleado_id) JOIN perfil_de_empleado P ON (P.empleado_id=E.ID) 
JOIN ubicacion U ON  (P.ubicacion_id=U.ID) LEFT JOIN baja_de_empleado B ON (B.EMPLEADO_ID=E.ID) 
WHERE C.ejercicio=$P{EJERCICIO} AND E.ID=$P{EMPLEADO_ID} AND S.tipo IN ('FALTA','INCAPACIDAD')
 AND (s.fecha>=e.alta or e.status='REINGRESO') and (b.fecha is null or (s.fecha<=b.fecha or e.alta>b.fecha))]]></queryString>

