	<parameter name="EMISOR_DIRECCION" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["BIOLOGO MAXIMINO MARTINEZ No. 3902 COL. SAN SALVADOR XOCHIMANCA DELEG. AZCAPOTZALCO MEXICO, D.F. C.P. 02870  TEL/FAX: 5342-7166 "]]></defaultValueExpression>
	</parameter>
	<parameter name="EJERCICIO" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT f.id,e.clave as clave_e,CONCAT(ifnull(E.apellido_paterno,'')," ",ifnull(E.apellido_materno,'')," ",E.nombres) AS nombre
,e.alta,date(f.date_created) as creado,date(f.last_updated) as ult_modif
,(SELECT MAX(x.nombre) FROM empresa x) as empresa
,(SELECT MAX(x.rfc) FROM empresa x) as rfc
,(SELECT MAX(x.registro_patronal) FROM empresa x) as registro
,(SELECT b.fecha FROM baja_de_empleado b where b.empleado_id=e.id) as baja
,(SELECT U.descripcion FROM ubicacion U WHERE p.ubicacion_id=u.id) AS ubicacion
,(SELECT U.descripcion FROM departamento U WHERE p.departamento_id=u.id) AS departamento,(SELECT U.descripcion FROM puesto U WHERE p.puesto_id=u.id) AS puesto
,f.anos_trabajados,f.antiguedad,f.dias_del_ejercicio,f.dias_para_aguinaldo,f.dias_aguinaldo
,f.vacaciones_anteriores,f.vacaciones_ejercicio,f.vacaciones_aplicadas,f.vacaciones_anteriores+f.vacaciones_ejercicio-f.vacaciones_aplicadas as vacs_pendtes,f.dias_trabajado_ejercicio,f.dias_trabajado_para_vacaciones,f.dias_por_pagar
,f.salario,f.salario_variable,f.salario_diario_integrado,f.salario*30 as sueldo_mensual
,f.indemnizacion_intereses,f.indemnizacion20dias_por_anio,f.indemnizacion3meses_de_sueldo,f.indemnizacion_prima_de_antiguedad
,x.tipo,x.clave,x.descripcion,sum(x.importe_gravado) as gravado,sum(x.importe_excento) as exento
,(case when (select ne.id from finiquito z join nomina_por_empleado ne on(ne.id=z.ne_liquidacion_id) where z.id=x.id) is null then 'FINIQUITO' else 'LIQUIDACION' end) as class
FROM (
SELECT f.id,'FINIQUITO' as class,substr(c.clave,1,1) AS tipo
,d.id as det_id,c.clave,c.descripcion,d.importe_gravado,d.importe_excento
FROM finiquito f 
join nomina_por_empleado ne on(ne.id=f.ne_finiquito_id)
join nomina n on(n.id=ne.nomina_id)
join nomina_por_empleado_det d on (d.parent_id=ne.id)
join concepto_de_nomina c on(c.id=d.concepto_id)
where n.ejercicio=$P{EJERCICIO}
union
SELECT f.id,'LIQUIDACION' as class,substr(c.clave,1,1) AS tipo
,d.id as det_id,c.clave,c.descripcion,d.importe_gravado,d.importe_excento
FROM finiquito f 
join nomina_por_empleado ne on(ne.id=f.ne_liquidacion_id)
join nomina n on(n.id=ne.nomina_id)
join nomina_por_empleado_det d on (d.parent_id=ne.id)
join concepto_de_nomina c on(c.id=d.concepto_id)
where n.ejercicio=$P{EJERCICIO}
) as x
join finiquito f on(f.id=x.id)
join empleado e on(e.id=f.empleado_id)
join perfil_de_empleado p on(p.empleado_id=e.id)
group by e.id,x.tipo,x.clave]]></queryString>

