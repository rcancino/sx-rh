	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="FECHA_INI" isForPrompting="false" class="java.util.Date"/>
	<parameter name="FECHA_FIN" isForPrompting="false" class="java.util.Date"/>
	<parameter name="EJERCICIO" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="EMPLEADO_ID" isForPrompting="true" class="java.lang.Long"/>
	<queryString><![CDATA[SELECT e.id,u.clave as ubicacion,concat(ifnull(e.apellido_paterno,''),' ',ifnull(e.apellido_materno,''),' ',ifnull(e.nombres,'')) as nombre,e.clave 
,e.alta,e.status,b.fecha as baja
,round((TO_DAYS((case when b.fecha is null or b.fecha<e.alta then current_date else b.fecha end))-TO_DAYS(e.alta))/(365),2) AS anios
,v.solicitud,DIAS_DATE,v.comentario,if(v.pg is true,'S','N') as pagado,t.clave as departamento
,c.dias_vacaciones,c.dias_trasladados,c.antiguedad_years,c.dias_tomados,c.dias_pagados
,(c.dias_vacaciones+c.dias_trasladados-c.dias_tomados-c.dias_pagados) as saldo
FROM vacaciones v join vacaciones_dias d on (d.vacaciones_id=v.id)
join empleado e on (v.empleado_id=e.id) join perfil_de_empleado p on (p.empleado_id=e.id) join ubicacion u on (p.ubicacion_id=u.id) 
join departamento t on (t.id=p.departamento_id) join control_de_vacaciones c on (c.id=v.control_id)
left join baja_de_empleado b on(b.empleado_id=e.id) 
 where year(dias_date)=$P{EJERCICIO} and e.id=$P{EMPLEADO_ID}]]></queryString>

