	<parameter name="FECHA_INI" isForPrompting="true" class="java.util.Date"/>
	<parameter name="FECHA_FIN" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT I.ID AS folio,CONCAT(E.apellido_paterno,' ',E.apellido_materno,' ',E.nombres) AS nombre,P.jornada
,I.fecha_inicial,I.fecha_final,ROUND((FLOOR(-(TIMESTAMPDIFF(MINUTE,I.fecha_final,I.fecha_inicial)/60)/24)+1),0) AS dias,I.tipo,I.comentario
,(case when i.pagado is true then 'SI' else 'NO' end) as pagado
,(SELECT U.descripcion FROM ubicacion U WHERE U.ID=P.ubicacion_id) AS ubicacion
,(SELECT U.descripcion FROM departamento U WHERE U.ID=P.departamento_id) AS departamento
,(SELECT x.nombre FROM empresa x ) as EMPRESA
FROM incidencia I JOIN empleado E ON(E.ID=I.empleado_id) 
JOIN perfil_de_empleado P ON(P.empleado_id=E.ID)
WHERE I.fecha_inicial>=$P{FECHA_INI} AND I.fecha_final<=$P{FECHA_FIN}]]></queryString>

