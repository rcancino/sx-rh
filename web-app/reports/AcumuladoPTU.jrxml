	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="NomReporte" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["ACUMULADO PTU"]]></defaultValueExpression>
	</parameter>
	<parameter name="EJERCICIO" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT CONCAT(ifnull(E.apellido_paterno,'')," ",ifnull(E.apellido_materno,'')," ",E.nombres) AS nombre,substr(s.periodicidad,1,1) as periodicidad
,ptd.ptu_excento,ptd.ptu_gravado,ptd.salario_diario,ptd.salario_mensual,ptd.incentivo,ptd.total_mensual_gravado
,ptd.tmg_isr,ptd.tmg_subsidio,ptd.tmg_resultado,ptd.smi_isr,ptd.smi_subsidio,ptd.smi_resultado,ptd.isr_por_retener,ptd.isr_acreditable,ptd.pensiona,ptd.prestamo,ptd.otras_ded,ptd.por_pagar_neto
,(case  when ptd.tipo=1 then '1.- PERSONAL QUE SE ENCUENTRA ACTUALMENTE LABORANDO CON DERECHO A PTU'
	when ptd.tipo=2 then '2.- PERSONAL QUE SE DIO DE BAJA CON DERECHO A PTU'
	when ptd.tipo=4 then '4.- PERSONAL QUE SE ENCUENTRA ACTUALMENTE LABORANDO SIN DERECHO A PTU POR HABER LABORADO MENOS DE 60 DIAS EN EL AÑO'
	when ptd.tipo=5 then '5.- PERSONAL QUE SE DIO DE BAJA SIN DERECHO A PTU POR HABER LABORADO MENOS DE 60 DIAS EN EL AÑO'	
	when ptd.tipo=3 then '3.- DIRECTORES Y GERENTES SIN DERECHO A PTU'
	else 'NO' end) as asignado
,(case when ptd.no_entregar is true then 'SI' else 'NO' end) as p_remanente
,pt.ejercicio,292281.42 as tope_sal_max_sind,pt.monto,pt.remanente,pt.monto_dias,pt.factor_dias,pt.monto_salario,pt.factor_salario,pt.monto+pt.remanente as total_ptu
,ptd.total as acum_anual,ptd.tope_anual,e.alta as f_ingreso,b.fecha as  f_baja,ptd.dias_del_ejercicio as dias_anio,ptd.faltas,ptd.incapacidades,ptd.dias_ptu,ptd.monto_dias as f_monto_dias,ptd.monto_salario as f_monto_salario
,ptd.por_pagar_bruto,e.status,p.ubicacion_id,u.clave
,pt.sindicalizado_maximo,pt.sindicalizado_nombre
,pt.salario_minimo_general,pt.tope_smg,ptd.vacaciones,ptd.retardos
,ptd.salario as sal,ptd.comisiones
FROM ptu_det ptd
JOIN PTU pt on(pt.id=ptd.ptu_id)
join empleado e on(e.id=ptd.empleado_id)
JOIN perfil_de_empleado P ON(E.ID=P.empleado_id)
join salario s on(s.empleado_id=e.id)
left join baja_de_empleado b on(b.empleado_id=e.id)
join ubicacion u on (u.id=p.ubicacion_id)
where pt.ejercicio=$P{EJERCICIO}
group by e.id
order by 21 ASC,2 DESC,44 ASC,1 ASC]]></queryString>

