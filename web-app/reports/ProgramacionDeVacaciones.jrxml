	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="EJERCICIO" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT substr(s.periodicidad,1,1) as per,u.clave as ubicacion,e.id,e.clave,concat(ifnull(e.apellido_paterno,''),' ',ifnull(e.apellido_materno,''),' ',ifnull(e.nombres,'')) as nombre,e.alta
,c.ejercicio,c.antiguedad_years,round((TO_DAYS(date(concat(ejercicio,'-01-01')))-TO_DAYS(e.alta))/((TO_DAYS(date(concat(ejercicio,'-12-31')))-TO_DAYS(date(concat(ejercicio,'-01-01'))))+1),2) AS anios
,c.dias_vacaciones,c.dias_trasladados,C.dias_tomados,C.dias_pagados
,c.dias_vacaciones+c.dias_trasladados-C.dias_tomados-C.dias_pagados as porTomar
,sum((case when vv.mes=1 then 1 else 0 end)) as ene
,sum((case when vv.mes=2 then 1 else 0 end)) as feb
,sum((case when vv.mes=3 then 1 else 0 end)) as mar
,sum((case when vv.mes=4 then 1 else 0 end)) as abr
,sum((case when vv.mes=5 then 1 else 0 end)) as may
,sum((case when vv.mes=6 then 1 else 0 end)) as jun
,sum((case when vv.mes=7 then 1 else 0 end)) as jul
,sum((case when vv.mes=8 then 1 else 0 end)) as ago
,sum((case when vv.mes=9 then 1 else 0 end)) as sep
,sum((case when vv.mes=10 then 1 else 0 end)) as oct
,sum((case when vv.mes=11 then 1 else 0 end)) as nov
,sum((case when vv.mes=12 then 1 else 0 end)) as dic
,(SELECT x.nombre FROM empresa x ) as EMPRESA
FROM (
SELECT v.empleado_id,month(d.dias_date) as mes,d.dias_date
FROM vacaciones v 
join vacaciones_dias d on(d.vacaciones_id=v.id) 
where d.dias_date BETWEEN date(concat($P{EJERCICIO},'-01-01')) and  date(concat($P{EJERCICIO},'-12-31'))
) as vv
join control_de_vacaciones c on(c.empleado_id=vv.empleado_id)
join empleado e on (c.empleado_id=e.id) join salario s on(s.empleado_id=e.id)
 join perfil_de_empleado p on (p.empleado_id=e.id) join ubicacion u on (p.ubicacion_id=u.id)
 where c.ejercicio=$P{EJERCICIO} and e.status<>'BAJA'
 group by e.id
 order by U.ID,e.alta]]></queryString>

