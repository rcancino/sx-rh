		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="NomReporte" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["REPORTE DE FALTAS E INCAPACIDADES"]]></defaultValueExpression>
	</parameter>
	<parameter name="FECHA_INICIAL" isForPrompting="true" class="java.util.Date"/>
	<parameter name="FECHA_FINAL" isForPrompting="true" class="java.util.Date"/>
	<parameter name="PERIODICIDAD" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
E.ID,U.CLAVE AS ubicacion,concat(ifnull(e.apellido_paterno,''),' ',ifnull(e.apellido_materno,''),' ',ifnull(e.nombres,'')) AS nombre,E.CLAVE AS EMP,P.numero_de_trabajador,S.FECHA,DAYOFWEEK(S.FECHA)AS DIA
,(case when dayofweek(S.fecha)=1 then 'DOM' when dayofweek(S.fecha)=2 then 'LUN' when dayofweek(S.fecha)=3 then 'MAR' when dayofweek(S.fecha)=4 then 'MIE' when dayofweek(S.fecha)=5 
then 'JUE' when dayofweek(S.fecha)=6 then 'VIE' when dayofweek(S.fecha)=7 then 'SAB' ELSE 'ND' END) as diaSem
,S.tipo,(SELECT DISTINCT(REFERENCIA_IMMS) FROM incapacidad I WHERE I.EMPLEADO_ID=E.ID AND S.FECHA BETWEEN I.FECHA_INICIAL AND I.FECHA_FINAL  ) AS REFERENCIA_IMMS
,1 AS COUNTER
, A.periodo_fecha_inicial, A.periodo_fecha_final
,(SELECT x.nombre FROM empresa x ) as EMPRESA
FROM
asistencia A  JOIN asistencia_det S ON (S.asistencia_id= A.ID )
JOIN empleado E ON (E.ID=A.empleado_id) JOIN perfil_de_empleado P ON (P.empleado_id=E.ID) 
JOIN ubicacion U ON  (P.ubicacion_id=U.ID) LEFT JOIN baja_de_empleado B ON (B.EMPLEADO_ID=E.ID) 
JOIN SALARIO L ON (E.ID=L.EMPLEADO_ID)
WHERE 
S.FECHA BETWEEN $P{FECHA_INICIAL} AND $P{FECHA_FINAL}  AND (s.fecha>=e.alta or e.status='REINGRESO')
AND S.tipo IN ('FALTA','INCAPACIDAD')  AND L.PERIODICIDAD=$P{PERIODICIDAD} and e.control_de_asistencia is true
and (b.fecha is null or (s.fecha<=b.fecha or e.alta>b.fecha)) 
ORDER BY u.id,E.clave,S.FECHA]]></queryString>

