	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="ID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="CALENDARIO_ID" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT C.asistencia_fecha_inicial,A.ID,E.STATUS,E.ALTA,E.id,E.clave,CONCAT(ifnull(E.apellido_paterno,"")," ",ifnull(E.apellido_materno,"")," ",E.nombres) AS nombre,P.numero_de_trabajador,P.jornada
,C.folio,C.id,a.tipo as tipoPrdo
,(SELECT U.descripcion FROM ubicacion U WHERE U.ID=P.ubicacion_id) AS ubicacion
,(SELECT U.descripcion FROM departamento U WHERE U.ID=P.departamento_id) AS depto
,A.ID,A.periodo_fecha_inicial,A.periodo_fecha_final,A.incapacidades,A.vacaciones
,A.retardo_menor as ret_,A.retardo_mayor,A.retardo_comida
,(case when dayofweek(D.fecha)=1 then 'DOM' when dayofweek(D.fecha)=2 then 'LUN' when dayofweek(D.fecha)=3 then 'MAR' when dayofweek(D.fecha)=4 then 'MIE' when dayofweek(D.fecha)=5 then 'JUE' when dayofweek(D.fecha)=6 then 'VIE' when dayofweek(D.fecha)=7 then 'SAB' ELSE 'ND' END) as diaSem
,D.id,d.fecha,D.entrada1,d.salida1,d.entrada2,d.salida2,d.entrada3,d.salida3,d.comentario
,CASE WHEN E.ALTA<=D.FECHA AND D.TIPO='FALTA' THEN 'FLT' WHEN (SELECT B.FECHA FROM baja_de_empleado B WHERE E.ID=B.EMPLEADO_ID)<D.FECHA AND D.TIPO='FALTA' THEN '' ELSE 
	((CASE WHEN d.tipo='FALTA' AND A.dias_trabajados>0  THEN '' 	WHEN d.tipo='FALTA' THEN 'FLT' WHEN d.tipo='DESCANSO' THEN 'DSC' WHEN d.tipo='DIA_FESTIVO' THEN 'FES' WHEN d.tipo='VACACIONES' THEN 'VAC' WHEN d.tipo='INCAPACIDAD' THEN 'INC'
		WHEN D.COMENTARIO='INCIDENCIA DESCANSO_P' THEN 'DES' WHEN D.COMENTARIO='INCIDENCIA PERMISO' THEN 'PCG' WHEN D.COMENTARIO='INCIDENCIA PERMISO_P' THEN 'PCP'  WHEN D.COMENTARIO='INCIDENCIA PATERNIDAD' THEN 'PAT'  		
		WHEN d.tipo='INCIDENCIA_F' THEN 'FLT' WHEN d.tipo='ASISTENCIA' THEN 'AST' ELSE 'NDT' END))
		END AS tipoReg
,a.horas_trabajadas,(case when d.horas_trabajadas>5 then d.horas_trabajadas-1 else d.horas_trabajadas end) as hrsTrab
,ifnull(((TIME_TO_SEC((CASE WHEN D.SALIDA2 IS NULL THEN SEC_TO_TIME((TIME_TO_SEC(d.salida1)-TIME_TO_SEC(d.entrada1))) ELSE SEC_TO_TIME((TIME_TO_SEC(d.salida2)-TIME_TO_SEC(d.entrada1))) END)))/60)/60,0) AS hrsTrabReal
,d.retardo_menor as retMn,d.retardo_mayor as retMy,d.retardo_comida as retCm
,A.dias_trabajados,(case when d.manual is true then '*' else '' end) as manual
,(SELECT x.nombre FROM empresa x ) as EMPRESA
FROM EMPLEADO E JOIN perfil_de_empleado P ON(E.ID=P.empleado_id) JOIN asistencia A ON(A.empleado_id=E.ID) JOIN asistencia_det D ON(A.ID=D.asistencia_id)
JOIN calendario_det C ON(C.id=A.calendario_det_id)
LEFT JOIN baja_de_empleado B ON(B.EMPLEADO_ID=E.ID)
WHERE E.ID LIKE $P{ID} AND C.ID=$P{CALENDARIO_ID} 
AND E.ALTA<=C.asistencia_fecha_final AND E.CONTROL_DE_ASISTENCIA IS TRUE  
and d.fecha<= (case when B.id IS not NULL AND b.fecha>e.alta and b.fecha<=d.fecha then b.fecha else d.fecha end)]]></queryString>

