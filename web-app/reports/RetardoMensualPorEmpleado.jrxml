	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="ID" isForPrompting="true" class="java.lang.String"/>
	<parameter name="FECHA_INI_PAGO" isForPrompting="true" class="java.util.Date"/>
	<parameter name="FECHA_FIN_PAGO" isForPrompting="true" class="java.util.Date"/>
	<parameter name="FECHA_INI" isForPrompting="true" class="java.util.Date"/>
	<parameter name="FECHA_FIN" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT E.id,E.clave,CONCAT("(",P.numero_de_trabajador,")"," ",e.clave," - ",ifnull(E.apellido_paterno,"")," ",ifnull(E.apellido_materno,"")," ",E.nombres) AS nombre,P.numero_de_trabajador,P.jornada,a.tipo as tipoPrdo
,(SELECT U.descripcion FROM ubicacion U WHERE U.ID=P.ubicacion_id) AS ubicacion
,(SELECT U.descripcion FROM departamento U WHERE U.ID=P.departamento_id) AS depto
,sum(d.retardo_menor) as retardo_menor,sum(case when d.retardo_menor>0 then 1 else 0 end) as regs_ret_min
,sum(d.retardo_menor_comida) as ret_men_com,sum(case when d.retardo_Menor_comida>0 then 1 else 0 end) as regs_ret_men_com
,(SELECT x.nombre FROM empresa x ) as EMPRESA
FROM EMPLEADO E 
JOIN perfil_de_empleado P ON(E.ID=P.empleado_id)
JOIN asistencia A ON(A.empleado_id=E.ID)
JOIN asistencia_det D ON(A.ID=D.asistencia_id)
WHERE A.empleado_id LIKE $P{ID} AND D.fecha between $P{FECHA_INI} and $P{FECHA_FIN} AND E.CLAVE<>'MARJ'  AND E.STATUS <>'BAJA' 
group by E.id,E.clave having sum(d.retardo_menor+d.retardo_menor_comida)>10]]></queryString>

