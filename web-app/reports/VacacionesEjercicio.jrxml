	<parameter name="FECHA_FIN" isForPrompting="false" class="java.util.Date"/>
	<parameter name="EJERCICIO" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT u.clave as ubicacion,e.id,concat(ifnull(e.apellido_paterno,''),' ',ifnull(e.apellido_materno,''),' ',ifnull(e.nombres,'')) as nombre,e.clave 
,c.dias_vacaciones,c.dias_trasladados,c.antiguedad_years,c.dias_tomados
,count(*) AS dias,v.solicitud
,(SELECT date(min(dias_date)) FROM vacaciones x join vacaciones_dias a on (a.vacaciones_id=x.id) where x.empleado_id=e.id) as fecha_inicial
,(SELECT date(max(dias_date)) FROM vacaciones x join vacaciones_dias a on (a.vacaciones_id=x.id) where x.empleado_id=e.id) as fecha_final
,if(v.pg is true,'S','N') as pagado
,t.clave as departamento
,v.comentario
FROM vacaciones v join vacaciones_dias d on (d.vacaciones_id=v.id)
join empleado e on (v.empleado_id=e.id) join perfil_de_empleado p on (p.empleado_id=e.id) join ubicacion u on (p.ubicacion_id=u.id)
join departamento t on (t.id=p.departamento_id)
join control_de_vacaciones c on (c.empleado_id=e.id)
 where year(dias_date)=$P{EJERCICIO}
 group by e.id
 order by U.ID,e.apellido_paterno,e.apellido_materno]]></queryString>

