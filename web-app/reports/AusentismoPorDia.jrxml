		<defaultValueExpression ><![CDATA["PAPEL S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="NomReporte" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["REPORTE DE AUSENTISMO"]]></defaultValueExpression>
	</parameter>
	<parameter name="FECHA" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT
1 AS COUNTER,
E.ID,C.FOLIO,C.INICIO,C.FIN,C.ASISTENCIA_FECHA_INICIAL,ASISTENCIA_FECHA_FINAL,U.CLAVE AS SUCURSAL,concat(ifnull(e.apellido_paterno,''),' ',ifnull(e.apellido_materno,''),' ',ifnull(e.nombres,'')) AS nombre,E.CLAVE AS EMP,P.numero_de_trabajador,D.FECHA,DAYOFWEEK(D.FECHA)AS DIA
,(SELECT ENTRADA1 FROM turno_det X WHERE X.turno_id=T.ID AND dias_idx=DAYOFWEEK(D.FECHA)-1) AS ENTRADA_TURNO
,(case when dayofweek(d.fecha)=1 then 'DOM' when dayofweek(d.fecha)=2 then 'LUN' when dayofweek(d.fecha)=3 then 'MAR' when dayofweek(d.fecha)=4 then 'MIE' when dayofweek(d.fecha)=5 then 'JUE' when dayofweek(d.fecha)=6 then 'VIE' when dayofweek(d.fecha)=7 then 'SAB' ELSE 'ND' END) as diaSem
,D.TIPO
,CASE WHEN E.ALTA>=C.asistencia_fecha_inicial THEN 'NUEVO' ELSE 'NORMAL' END AS TIPO_EMPLEADO
,B.FECHA AS FECHA_BAJA,E.ALTA
,case when b.fecha is null then asistencia_fecha_final
      when b.fecha <=e.alta then asistencia_fecha_final else b.fecha end as final
,case when e.alta  between C.ASISTENCIA_FECHA_INICIAL and asistencia_fecha_final then e.alta else asistencia_fecha_inicial end as ini
,(SELECT x.nombre FROM empresa x ) as EMPRESA
FROM
asistencia_det D JOIN asistencia A ON (A.id=D.asistencia_id) JOIN empleado E ON (E.ID=A.empleado_id) JOIN perfil_de_empleado P ON (P.empleado_id=E.ID) JOIN ubicacion U ON  (P.ubicacion_id=U.ID)
JOIN turno T ON ( P.turno_id=T.ID)  JOIN calendario_det C ON (C.ID=A.CALENDARIO_DET_ID)
LEFT JOIN baja_de_empleado B ON (B.EMPLEADO_ID=E.ID)
WHERE d.fecha=$P{FECHA}
AND  d.TIPO NOT IN ('ASISTENCIA')
and e.control_de_asistencia is true
ORDER BY U.ID,e.clave]]></queryString>

